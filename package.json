{
  "name": "campaign-copilot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "tsc -b && vite build",
